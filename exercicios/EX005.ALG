algoritmo "EX005"
var
   grade1, grade2, average: Real
inicio
      Escreva("Digite a primeira nota: ")
      Leia(grade1)
      Escreva("Digite a segunda nota: ")
      Leia(grade2)
      average <- (grade1 + grade2) / 2
      Escreval("A média entre",grade1," e",grade2," é igual a",average,".")
fimalgoritmo
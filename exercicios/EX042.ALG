algoritmo "EX042"
var
   numero, contador: Inteiro
inicio
   Escreva("Informe um número inteiro e positivo: ")
   Leia(numero)
   contador <- 1
   EscrevaL()
   Escreva("Contagem:")
   Enquanto (contador <= numero) faca
      Escreva(contador)
      contador <- contador + 1
   FimEnquanto
   Escreva(" Acabou!")
   EscrevaL()
fimalgoritmo
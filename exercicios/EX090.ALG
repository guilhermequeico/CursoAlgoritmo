algoritmo "EX090"
var
   num1, num2: Inteiro
Procedimento Somador(num1, num2: Inteiro)
Inicio
   EscrevaL("Resultado:", num1 + num2)
FimProcedimento
inicio
   Escreva("Informe um valor: ")
   Leia(num1)
   Escreva("Informe outro valor: ")
   Leia(num2)
   Somador(num1, num2)
fimalgoritmo